/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page {
  width: 100%;
  height: 100%;
  overflow-x: hidden;
  background-color: #f2f2f2;
  font-family: ff-regular;
  /* overflow-y: auto; */
  /* font-family: 'Source Han Sans CN'; */
}

